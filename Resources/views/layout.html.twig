{% extends "AsmTranslationLoaderBundle::base.layout.html.twig" %}

{% block head %}
    <link rel="icon" sizes="16x16" href="{{ asset('favicon.ico') }}" />
    <link rel="stylesheet" href="{{ asset('bundles/asmtranslationloader/css/default.css') }}" />

    <script src="{{ asset('bundles/asmtranslationloader/js/default.js') }}" type="text/javascript"></script>
{% endblock %}

{% block title %}
    {{ 'TranslationLoaderBundle'|trans }}
{% endblock %}

{% block body %}
<div class="navbar navbar-default navbar-fixed-top" role="navigation">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">{{ 'Toggle navigation'|trans }}</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">{{ 'TranslationLoaderBundle'|trans }}</a>
        </div>
        <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li><a href="#">{{ 'List'|trans }}</a></li>
                <li><a href="#">{{ 'Add'|trans }}</a></li>
            </ul>
        </div>
    </div>
</div>
<div class="container">
    {% block content %}{% endblock %}
</div>
{% endblock %}
